section .data
    odd_msg db "Odd number", 10
    odd_len equ $ - odd_msg

    even_msg db "Even number", 10
    even_len equ $ - even_msg

    num dd 7     

section .text
    global _start

_start:
    mov eax, [num]     
    test eax, 1        
    jz even         

odd:
    mov eax, 4        
    mov ebx, 1     
    mov ecx, odd_msg
    mov edx, odd_len
    int 0x80
    jmp exit

even:
    mov eax, 4
    mov ebx, 1
    mov ecx, even_msg
    mov edx, even_len
    int 0x80

exit:
    mov eax, 1       
    xor ebx, ebx     
    int 0x80
